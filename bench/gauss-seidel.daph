
A = rand(100, 100, 0.0, 1.0, 1, -1);
b = rand(100, 1, 0.0, 1.0, 1, -1);

//implement gauss seidel method in matrix form
//A = L + D + U
L = fill(0.0, 100, 100);
D = fill(0.0, 100, 100);
U = fill(0.0, 100, 100);

for(i in 0:99) {
	for(j in 0:99) {
		if(i == j) {
			D[i, j] = A[i, j];
		}
		else if(i > j) {
			L[i, j] = A[i, j];
		}
		else {
			U[i, j] = A[i, j];
		}
	}
}

